{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "allowedPostUpgradeCommands": ["bin/generate_files"],
  "recreateClosed": true,
  "git-submodules": {
    "enabled": true
  },
  "postUpgradeTasks": {
    "commands": ["bin/generate_files"],
    "fileFilters": ["lib/**"],
    "executionMode": "update"
  }
}
